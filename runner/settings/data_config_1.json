{
  "stocks_download": {
    "type": "populate stocks in exchange",
    "config":{
      "exchanges": ["nyse"],
      "instruments": ["stock"]
    }
  },
  "stocks_historical_data_download": {
    "type": "data download",
    "config": {
      "stocks": [
        "PAYC", "SHOP", "TTS"
      ],
      "data": {
          "type": "historical",
          "params":
          {
            "start_date": "2012-01-01",
            "end_date": "2019-10-22",
            "bar_size":  "1 day",
            "price_type": "TRADES"
          }
      }
    }
  },
  "stocks_financial_data_download": {
    "type": "data download",
    "config": {
      "stocks": [
        "PAYC", "SHOP", "TTS"
      ],
      "data": {
        "type": "fundamental",
        "params": {
          "report_types": [
            "ReportsFinSummary",
            "ReportsOwnership",
            "ReportSnapshot",
            "ReportsFinStatements",
            "RESC",
            "CalendarReport"
          ]
        }
      }
    }
  }
}